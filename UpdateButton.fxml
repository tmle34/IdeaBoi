               <font>
                  <Font name="System Bold Italic" size="17.0" />
               </font>
            </Label>
            <Button layoutX="180.0" layoutY="242.0" mnemonicParsing="false" text="Enter" />
            <VBox layoutX="272.0" layoutY="100.0" prefHeight="200.0" prefWidth="189.0">
               <children>
                  <TextArea prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
            <Label layoutX="273.0" layoutY="63.0" text="Updated:">
               <font>
                  <Font name="System Bold Italic" size="17.0" />
